{
  "compilerOptions": {
    "allowJs": true,
    "checkJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "isolatedModules": true,
    "jsx": "react",
    "lib": [
      "es6"
    ],
    "moduleResolution": "node",
    "noEmit": true,
    "strict": true,
    "target": "esnext",
    "noImplicitAny": false,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@app": ["./src/app/index"],
      "@app/*": ["./src/app/*"],
      "@components": ["./src/app/components/index"],
      "@components/*": ["./src/app/components/*"],
      "@navigation": ["./src/app/navigation/index"],
      "@navigation/*": ["./src/app/navigation/*"],
      "@network": ["./src/app/network/index"],
      "@network/*": ["./src/app/network/*"],
      "@model/*": ["./src/app/model/*"],
      "@screen": ["./src/app/Screen/index"],
      "@screen/*": ["./src/app/Screen/*"],
      "@util/*": ["./src/app/util/*"],
      "@assets/*": ["./src/assets/*"],
      "@store/*": ["./src/app/store/*"],
      "@styled-components/native":["styled-components/native"]
    }
  },
  "exclude": [
    "node_modules",
    "babel.config.js",
    "metro.config.js",
    "jest.config.js"
  ]
}
